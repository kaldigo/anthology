<MudDialog>
    <DialogContent>
        <MudTextField @bind-Value="seriesName" Label="Name" Variant="Variant.Text" Margin="Margin.Normal" />
        <MudNumericField @bind-Value="seriesSequence" Label="Sequence" Variant="Variant.Text" Margin="Margin.Normal" />
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Primary" OnClick="Add">Add</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }
    private string seriesName;
    private float seriesSequence;

    void Add() => MudDialog.Close(DialogResult.Ok(new KeyValuePair<string,float>(seriesName, seriesSequence)));
    void Cancel() => MudDialog.Cancel();
}