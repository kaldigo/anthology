<MudDialog>
    <DialogContent>
        <MudTextField @bind-Value="itemString" Label="@itemName" Variant="Variant.Text" Margin="Margin.Normal" />
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Primary" OnClick="Add">Add</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }
    [Parameter] public string itemName { get; set; }
    private string itemString = "";

    void Add() => MudDialog.Close(DialogResult.Ok(itemString));
    void Cancel() => MudDialog.Cancel();
}